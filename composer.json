{
    "name": "co-developers/wp-skeleton",
    "type": "project",
    "license": "MIT",
    "description": "WordPress Skeleton con herramientas modernas de desarrollo, fácil configuración, mayor seguridad, y una estructura de carpetas mas ordenada",
    "homepage": "http://co-developers.com",
    "authors": [
        {
            "name": "Edgardo Garofalo",
            "email": "egarofalo83@hotmail.com",
            "homepage": "https://github.com/egarofalo",
            "role": "Developer"
        }
    ],
    "keywords": [
        "wp-skeleton", "co-developers", "wordpress", "wordpress skeleton", "composer", "wp"
    ],    
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        }
    ],
    "require": {
        "php": ">=5.4",
        "johnpbloch/wordpress": "^4.8",
        "symfony/yaml": "^3.3.5",
        "wpackagist-theme/twentyseventeen": "^1.3"
    },
    "extra": {
        "wordpress-install-dir": "wp"
    },
    "autoload": {
        "psr-4": {
            "CoDevelopers\\WpSkeleton\\Config\\": "config"
        }
    },    
    "scripts": {
        "post-create-project-cmd": "CoDevelopers\\WpSkeleton\\Config\\ComposerSetup::postCreateProject",
        "post-update-cmd": "CoDevelopers\\WpSkeleton\\Config\\ComposerSetup::postUpdate",
        "post-install-cmd": "CoDevelopers\\WpSkeleton\\Config\\ComposerSetup::postInstall"
    }
}
